# Среда: test | production (по умолчанию production)
VITE_APP_ENV=test

# Максимальный размер загружаемого файла в МБ (по умолчанию 100)
VITE_MAX_FILE_SIZE_MB=100

# ============================================================
# PRODUCTION — рабочая среда
# ============================================================

VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-publishable-default-key
VITE_SUPABASE_TENANT_ID=your-tenant-id
VITE_OPENROUTER_API_KEY=your-openrouter-api-key

# Провайдер хранилища: cloudru | cloudflare
VITE_STORAGE_PROVIDER=cloudru

# Cloud.ru S3 (S3-совместимое хранилище)
VITE_S3_ENDPOINT=https://your-endpoint.cloud.ru
VITE_S3_REGION=ru-msk
VITE_S3_ACCESS_KEY=your-access-key
VITE_S3_SECRET_KEY=your-secret-key
VITE_S3_BUCKET=your-bucket-name

# Cloudflare R2 (S3-совместимое хранилище)
VITE_R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
VITE_R2_ACCESS_KEY=your-r2-access-key
VITE_R2_SECRET_KEY=your-r2-secret-key
VITE_R2_BUCKET=your-r2-bucket-name

# ============================================================
# TEST — тестовая среда (используется при VITE_APP_ENV=test)
# ============================================================

VITE_TEST_SUPABASE_URL=https://your-test-project.supabase.co
VITE_TEST_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your-test-publishable-default-key
VITE_TEST_SUPABASE_TENANT_ID=your-test-tenant-id
VITE_TEST_OPENROUTER_API_KEY=your-test-openrouter-api-key

# Cloud.ru S3 (тестовое)
VITE_TEST_S3_ENDPOINT=https://your-test-endpoint.cloud.ru
VITE_TEST_S3_REGION=ru-msk
VITE_TEST_S3_ACCESS_KEY=your-test-access-key
VITE_TEST_S3_SECRET_KEY=your-test-secret-key
VITE_TEST_S3_BUCKET=your-test-bucket-name

# Cloudflare R2 (тестовое)
VITE_TEST_R2_ENDPOINT=https://your-test-account-id.r2.cloudflarestorage.com
VITE_TEST_R2_ACCESS_KEY=your-test-r2-access-key
VITE_TEST_R2_SECRET_KEY=your-test-r2-secret-key
VITE_TEST_R2_BUCKET=your-test-r2-bucket-name
